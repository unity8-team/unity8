    "framework": "ubuntu-sdk-13.10",
    "architecture": "armhf",
    "hooks": {
        "@APP_NAME@": {
            "apparmor": "apparmor.json",
            "desktop": "@DESKTOP_FILE@"
        }
    },
    "maintainer": "Ubuntu Reminders app developers <ubuntu-phone@lists.launchpad.net>",
    "name": "@PROJECT_NAME@",
    "title": "Reminders",
    "version": "0.4.@BZR_REVNO@",
